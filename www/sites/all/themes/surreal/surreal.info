name = Surreal
description = Premium theme for Drupal 7.
core = 7.x
engine = phptemplate

;CSS Styles
stylesheets[all][] = css/skeleton.css
stylesheets[all][] = css/prettyPhoto.css
stylesheets[all][] = css/inspiromedia.css

;JavaScripts
scripts[] = js/prefixfree.min.js
scripts[] = js/modernizr-2.6.2.js
scripts[] = js/iOS-timer.js
scripts[] = js/jquery-1.8.3.min.js
scripts[] = js/jquery.prettyPhoto.js
scripts[] = js/jquery.isotope.min.js
scripts[] = js/jquery.isotope.run.js
scripts[] = js/jquery.easing.min.js
scripts[] = js/jquery.mobile-touch-swipe-1.0.js
scripts[] = js/jquery.flexslider.js
scripts[] = js/jquery.parallax-1.1.3.js
scripts[] = js/contact_form.js
scripts[] = js/tooltip.js
scripts[] = js/jquery.sticky.js

;Regions
regions[hero]			= Hero
regions[region1] 		= About Us #1
regions[region2] 		= Services #2
regions[region3] 		= Our People #3
regions[region4] 		= Portfolio #4
regions[region5]		= Careers #5
regions[region6] 		= Contact Us #6
regions[footer]      	= Footer
regions[copyright]      = Copyright
regions[content] 		= Content
regions[sidebar] 		= Sidebar


;Settings & Variables
settings[menu_element] = main-menu
variables[admin_theme] = 'seven'







